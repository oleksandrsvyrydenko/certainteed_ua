# Staging Environment Authentication Credentials
# Copy this file to .env and fill in your actual credentials

# Basic Authentication
STAGING_USERNAME=your_staging_username
STAGING_PASSWORD=your_staging_password

# Bearer Token Authentication (if using JWT/OAuth)
STAGING_ACCESS_TOKEN=your_access_token_here

# API Key Authentication (if using API keys)
STAGING_API_KEY=your_api_key_here

# Additional Headers (if needed)
STAGING_CUSTOM_HEADER=your_custom_header_value

